<script>
var anim_abstract = ['../animations/abstract/20120824-2355_series2.mp4', 
'../animations/abstract/20120824-2356_series3.mp4', 
'../animations/abstract/20120824-2357_series4.mp4', 
'../animations/abstract/20120824-2357_series5.mp4',
'../animations/abstract/20120825-0000_series6.mp4'];

      anim_abstract.sort(function (a, b) {
      return Math.random() > 0.5 ? -1 : 1;
      });

      var curVideo_01 = 0;
      var videoPlayer_01 = document.getElementById('videoplayer_anim_abstract');

      videoPlayer_01.onended = function(){
  		    ++curVideo_01;
          if(curVideo_01 < anim_abstract.length){
        videoPlayer_01.src = anim_abstract[curVideo_01];
      }
  }
</script>

